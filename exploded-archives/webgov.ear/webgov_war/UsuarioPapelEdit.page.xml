<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/UsuarioPapelList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{usuarioPapelHome.wire}"/>

   <param name="usuarioPapelFrom"/>
   <param name="usuarioPapelIdPapel" value="#{usuarioPapelHome.usuarioPapelId.idPapel}"/>
   <param name="usuarioPapelIdUsuario" value="#{usuarioPapelHome.usuarioPapelId.idUsuario}"/>
   <param name="papelFrom"/>
   <param name="papelId" value="#{papelHome.papelId}"/>

   <param name="usuarioFrom"/>
   <param name="usuarioId" value="#{usuarioHome.usuarioId}"/>



   <navigation from-action="#{usuarioPapelHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/UsuarioPapel.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{usuarioPapelHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/UsuarioPapel.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{usuarioPapelHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/UsuarioPapelList.xhtml"/>
      </rule>
   </navigation>

</page>
